{
  "name": "ebash",
  "private": false,
  "version": "1.7.1",
  "description": "Утилита для frontend разработки",
  "license": "MIT",
  "repository": "ssh://git@gitlab.tochka-tech.com:2224/frontend-core/ebash.git",
  "bin": {
    "ebash": "./console.js"
  },
  "publishConfig": {
    "registry": "http://nexus.bank24.int/repository/tochka-modules/"
  },
  "scripts": {
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node lib/console.js",
    "prebuild": "rimraf dist",
    "build": "nest build && chmod +x ./dist/console.js && cp ./package.json ./dist/package.json && yarn copy_data",
    "copy_data": "cp -rf ./src/console-services/make/templates/ ./dist/console-services/make/templates/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@babel/parser": "^7.7.7",
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/platform-express": "^6.10.14",
    "commander": "^4.1.1",
    "inquirer": "7.0.5",
    "json2csv": "^5.0.0",
    "json2md": "^1.7.0",
    "lodash.template": "^4.5.0",
    "module-loader-tool": "0.1.4",
    "moment": "^2.24.0",
    "nestjs-console": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "tslint": "^6.1.2",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
